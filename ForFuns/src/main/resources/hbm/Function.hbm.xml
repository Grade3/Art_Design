<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<query name="FunctionqueryGetAllTopMenu">  
        <![CDATA[from FunctionBean where istopmenu = 1]]>
	</query>
	
	<query name="FunctionqueryGetAllSecondMenu">  
        <![CDATA[from FunctionBean where istopmenu = 0]]>
	</query>
</hibernate-mapping>  